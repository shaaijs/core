  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shaaijs/shaai/issues"
  },
  "homepage": "https://github.com/shaaijs/shaai#readme",
  "dependencies": {
    "medium-json-feed": "0.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "rimraf": "^2.6.3"
  }
